# for drupal 8
# Ignore directories generated by Composer.
/drush/contributed
/drush/contrib
/api/vendor
/api/core
/api/modules/contributed
/api/modules/contrib
/api/themes/contributed
/api/themes/contrib
/api/profiles/contributed
/api/profiles/contrib
/api/libraries

# Ignore generated assets.
/api/modules/custom/**/scripts/compiled
/api/modules/custom/**/stylesheets/compiled
/api/themes/custom/**/scripts/compiled
/api/themes/custom/**/stylesheets/compiled

# Ignore local configuration overrides.
/api/sites/**/settings.local.php
/configuration.local.json

# Ignore Drupal's file directory.
/api/sites/*/files

# Ignore SimpleTest multi-site environment.
/api/sites/simpletest

# Ignore Drupal's scaffolding.
/api/.csslintrc
/api/.editorconfig
/api/.eslintignore
/api/.eslintrc.json
/api/.gitattributes
/api/.ht.router.php
/api/.htaccess
/api/autoload.php
/api/index.php
/api/robots.txt
/api/update.php
/api/api.config

# Ignore the build stub index.html
/api/index.html

.DS_Store
node_modules
/dist

/tests/e2e/videos/
/tests/e2e/screenshots/

# local env files
.env.local
.env.*.local

# Log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Editor directories and files
.idea
.vscode
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw*
