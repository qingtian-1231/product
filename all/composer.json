  "conflict": {
    "drupal/core": "8.*"
  },
  "scripts": {},
  "config": {
    "sort-packages": false,
    "platform": {
      "php": "7.1.2"
    },
    "vendor-dir": "vendor",
    "discard-changes": "stash",
    "optimize-autoloader": true,
    "preferred-install": {}
  },
  "autoload": {
    "psr-0": {
      "UIComposerHooks\\": "hooks/"
    },
    "psr-4": {}
  },
  "extra": {
    "merge-plugin": {
      "require": [],
      "merge-extra": true,
      "merge-extra-deep": true
    },
    "drupal-libraries": {
      "joyride": "https://github.com/zurb/joyride/archive/v2.0.3.zip",
      "phpsass": "https://github.com/richthegeek/phpsass/archive/2014-03-20.zip",
      "simplehtmldom": "https://downloads.sourceforge.net/project/simplehtmldom/simplehtmldom/1.5/simplehtmldom_1_5.zip?use_mirror=superb-dca2"
    },
    "installer-types": [],
    "installer-paths": {
      "vendor/drupal/drupal": [
        "type:drupal-core"
      ],

      "drush/{$name}": [
        "type:drupal-drush",
        "drupal/casperjs",
        "drupal/composer",
        "drupal/drush_sql_sync_pipe",
        "drupal/drush_entity",
        "drupal/httprl_spider",
        "drupal/registry_rebuild",
        "drupal/site_audit"
      ],

      "modules/contributed/{$name}": [
        "type:drupal-module"
      ],
      "modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],

      "themes/contributed/{$name}": [
        "type:drupal-theme"
      ],
      "themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ],

      "libraries/aws-sdk-for-php": [
        "drupal/awssdk_library"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],

      "profiles/contributed/{$name}": [
        "type:drupal-profile"
      ]
    },
    "patches": {
      "drupal/drupal": {
        "Certain URL fragments cause javascript error when collapsible fieldset is present on page (https://www.drupal.org/node/2395065)": "patches/collapse.patch"
      },
      "drupal/autologout": {
        "Automatically log out anonymous users": "patches/autologout.patch"
      },
      "drupal/cdn": {
        "advagg (https://www.drupal.org/project/advagg) module hooks": "https://www.drupal.org/files/cdn-1942230-18-advagg-hooks.patch",
        "Remove file_create_url function when processing css background images": "patches/cdn-css-aggregation.patch"
      },
      "drupal/ctools": {
        "Modal windows": "patches/ctools.patch"
      },
      "drupal/commerce": {
        "Commerce checkout": "patches/commerce_checkout.patch"
      },
      "drupal/commerce_paypal": {
        "Order status update patch": "patches/commerce_paypal_checkout_complete.patch"
      },
      "drupal/commerce_shipping": {
        "Fix for shipping rates recalculation": "patches/commerce_shipping.patch"
      },
      "drupal/disclaimer": {
        "Custom cookie name.": "https://www.drupal.org/files/issues/does-not-work-with-custom-cookie-name-2499005.patch"
      },
      "drupal/features": {
        "array_diff_assoc()": "https://www.drupal.org/files/1588596-11-features-array_diff_assoc_recursive.patch"
      },
      "drupal/file_entity": {
        "Use correct S3 URLs": "https://www.drupal.org/files/issues/2479483.2-uri-alter.patch"
      },
      "drupal/memcache_storage": {
        "Initialize session object": "https://www.drupal.org/files/issues/initialize_session_object-2348955.patch"
      },
      "drupal/multifield": {
        "Allow for image field to be empty": "patches/multifield.patch",
        "Call to undefined function multifield_extract_multifield_machine_name() (https://www.drupal.org/project/multifield/issues/2546900)": "https://www.drupal.org/files/issues/multifield-2546900-include-module-file-2.patch",
        "Recoverable fatal error: Argument 1 passed to multifield_extract_multifield_machine_name() must be of the type array, null given (https://www.drupal.org/project/multifield/issues/2451011)": "https://www.drupal.org/files/issues/multifield-2451011-3.patch"
      },
      "drupal/print": {
        "Customer proxy rewrite, email validation etc.": "patches/print.patch"
      },
      "drupal/search_api": {
        "Summary, score error suppression": "patches/search_api.patch"
      },
      "drupal/stage_file_proxy": {
        "Add an option to skip SSL verification (https://www.drupal.org/project/stage_file_proxy/issues/3008351)": "https://www.drupal.org/files/issues/2019-04-26/stage_file_proxy-add-verify-option-3008351-2.patch"
      },
      "drupal/views": {
        "Sorting, sort orders, theme hook suggestions": "patches/views.patch"
      },
      "dompdf/dompdf": {
        "Version (exact version is required by the print module)": "patches/dompdf.VERSION.patch"
      },
      "drupal/mail_settings_checker": {
        "Removing the hardcoded file locations": "patches/mail_settings_checker.patch"
      }
    },
    "composer-exit-on-patch-failure": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
