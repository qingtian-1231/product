{
    "name": "symplify/better-phpdoc-parser",
    "description": "Slim wrapper around phpstan/phpdoc-parser with format preserving printer",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "nette/utils": "^2.5",
        "phpstan/phpdoc-parser": "^0.3.1",
        "symplify/package-builder": "^5.4.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\BetterPhpDocParser\\": "src",
            "Symplify\\BetterPhpDocParser\\Attributes\\": "packages/Attributes/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\BetterPhpDocParser\\Tests\\": "tests",
            "Symplify\\BetterPhpDocParser\\Attributes\\Tests\\": "packages/Attributes/tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
