{
  "name": "product",
  "description": "product.",
  "type": "project",
  "support": {},
  "keywords": [],
  "license": "proprietary",
  "authors": [
    {
      "name": "CPQ",
      "role": "Chief architect"
    }
  ],
  "repositories": {
    "drupal_packagist": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "packagist": {
      "type": "composer",
      "url": "https://packagist.org/"
    },

    "drush_cmi_tools": {
      "type": "git",
      "url": "https://github.com/previousnext/drush_cmi_tools.git"
    }
  },
  "__comments": [
    "Drupal Packagist: https://packagist.phpcomposer.com",
    "drupal/node_export is at '1.x-dev#b568078d75ed9d0637200cafbc04580cc63af3a7' due to breaking changes in more recent commits.",
    "phpmd/phpmd requirement is at 'dev-master' because new releases have not been tagged for a while; set back at '^2.0' at some point.",
    "phploc/phploc requirement is at '^3.0' because '^4.0' conflicts with phpunit/phpunit.",
    "sebastian/phpcpd requirement is at '^3.0' because '^4.0' conflicts with phpunit/phpunit."
  ],
  "require": {
    "php": "^7.1",
    "ext-curl": "*",
    "ext-json": "*",

    "hirak/prestissimo": "^0.3.0",
    "zaporylie/composer-drupal-optimizations": "^1.0",

    "composer/installers": "^1.0",
    "oomphinc/composer-installers-extender": "^1.0",
    "balbuf/drupal-libraries-installer": "^1.0",
    "wikimedia/composer-merge-plugin": "^1.0",
    "cweagans/composer-patches": "^1.0",

    "drupal-composer/drupal-scaffold": "^2.5",

    "drupal/console": "^1.5",

    "drupal/core": "~8.8.1",

    "drupal/config_installer": "^1.5",

    "drush/drush": "^8.0",
    "drush/config-extra": "^1.0.0",

    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3",
    "vlucas/phpdotenv": "^3.0.0",
    "drupal/module_filter": "^3.1",
    "drupal/restui": "^1.17",
    "drupal/commerce": "^2.15",
    "drupal/admin_toolbar": "^2.0",
    "drupal/jsonapi_extras": "^3.12",
    "drupal/rest_menu_items": "^2.9",
    "drupal/simple_oauth": "^4.3",
    "drupal/fontawesome_menu_icons": "^1.5",
    "drupal/examples": "^1.0",
    "drupal/field_group": "^3.0",
    "drupal/search_api": "^1.15",
    "drupal/devel": "^2.1",
    "drupal/paragraphs": "^1.10",
    "drupal/weight": "^3.1",
    "drupal/backup_migrate": "^4.1",
    "drupal/webform": "^5.8",
    "drupal/webform_rest": "^2.0@beta",
    "drupal/commerce_cart_api": "^1.4",
    "drupal/cors": "^1.0",
    "drupal/file_download": "^1.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",

    "sensiolabs/security-checker": "^5.0.0",

    "dealerdirect/phpcodesniffer-composer-installer": "^0.5",

    "behat/mink": "1.7.x-dev",
    "behat/mink-goutte-driver": "^1.2",
    "behat/mink-selenium2-driver": "1.3.x-dev",
    "jcalderonzumba/gastonjs": "^1.0.2",
    "jcalderonzumba/mink-phantomjs-driver": "^0.3.1",
    "mikey179/vfsstream": "^1.2",
    "phpunit/phpunit": "^4.8.35 || ^6.5",
    "phpspec/prophecy": "^1.7",
    "symfony/css-selector": "^3.4.0",
    "symfony/phpunit-bridge": "^3.4.0",
    "symfony/debug": "^3.4.0",

    "fenetikm/autoload-drupal": "^0.2",

    "jakub-onderka/php-parallel-lint": "^1.0",
    "jakub-onderka/php-var-dump-check": "^0.3",
    "jakub-onderka/php-console-highlighter": "^0.3",
    "squizlabs/php_codesniffer": "^3.0",
    "escapestudios/symfony2-coding-standard": "^3.0",
    "slevomat/coding-standard": "^5.0",
    "symplify/easy-coding-standard": "^5.0",
    "object-calisthenics/phpcs-calisthenics-rules": "^3.0",
    "phpcompatibility/php-compatibility": "^9.0",
    "phpcompatibility/phpcompatibility-symfony": "^1.0",
    "phpcompatibility/phpcompatibility-paragonie": "^1.0",
    "phpcompatibility/phpcompatibility-passwordcompat": "^1.0",
    "friendsofphp/php-cs-fixer": "^2.0",
    "phpmd/phpmd": "dev-master",
    "povils/phpmnd": "^2.0",
    "sebastian/phpcpd": "^3.0",
    "phploc/phploc": "^4.0",
    "phan/phan": "^1.0",
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "mglaman/phpstan-drupal-deprecations": "^0.11",
    "vimeo/psalm": "^3.0.0",
    "alexia/php7mar": "^0.2.0-beta",
    "mglaman/drupal-check": "^1.0"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "scripts": {
    "pre-install-cmd": [
      "@pre-install-update"
    ],
    "pre-update-cmd": [
      "@pre-install-update"
    ],
    "post-install-cmd": [
      "@post-install-update"
    ],
    "post-update-cmd": [
      "@post-install-update"
    ],
    "pre-install-update": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-update": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ],
    "install-coding-standards": [
      "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
    ]
  },
  "config": {
    "sort-packages": false,
    "platform": {
      "php": "7.1.3"
    },
    "vendor-dir": "api/vendor",
    "discard-changes": "stash",
    "optimize-autoloader": true
  },
  "autoload": {},
  "extra": {
    "merge-plugin": {
      "require": [
        "api/modules/custom/pro_core/composer.json"
      ],
      "merge-extra": true,
      "merge-extra-deep": true
    },
    "drupal-libraries": {
      "d3": "https://github.com/d3/d3/archive/v3.5.17.zip",
      "highlightjs": "https://github.com/components/highlightjs/archive/9.12.0.zip",
      "urijs": "https://github.com/medialize/URI.js/archive/gh-pages.zip",
      "fontawesome-iconpicker": "https://github.com/itsjavi/fontawesome-iconpicker/archive/3.0.0.zip"
    },
    "installer-types": [],
    "installer-paths": {
      "api/core": [
        "type:drupal-core"
      ],

      "drush/contributed/{$name}": [
        "type:drupal-drush",
        "drupal/drush_sql_sync_pipe",
        "drupal/site_audit"
      ],

      "api/modules/contributed/{$name}": [
        "type:drupal-module",
        "drupal/node_export",
        "drupal/coder"
      ],
      "api/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],

      "api/themes/contributed/{$name}": [
        "type:drupal-theme"
      ],
      "api/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ],

      "api/libraries/{$name}": [
        "type:drupal-library"
      ],

      "api/profiles/contributed/{$name}": [
        "type:drupal-profile"
      ]
    },
    "patches": {
      "drupal/core": {
        "Symlinks in files directory (https://www.drupal.org/project/drupal/issues/1008402)": "https://www.drupal.org/files/issues/stream_wrappers-allow_symlinks-1008402-115.patch"
      },
      "drupal/libraries": {
        "Fatal error: Cannot declare interface Drupal\\libraries\\ExternalLibrary\\Utility\\LibraryAccessorIdInterface (https://www.drupal.org/project/libraries/issues/2882709)": "https://www.drupal.org/files/issues/2882709-2-interface-libraryidaccessorinterface-misnamed.patch"
      },
      "drupal/node_revision_delete": {
        "Missing group by in NodeRevisionDelete patche one": "https://www.drupal.org/files/issues/2019-09-18/node_revision_delete-fix_candidate_nodes_group_by-3082021-2.patch",
        "Missing group by in NodeRevisionDelete patche two": "https://www.drupal.org/files/issues/2019-10-08/3078433-5.patch"
      }
    },
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "composer-exit-on-patch-failure": true,
    "autoload-drupal": {
      "modules": [
        "api/core/modules/",
        "api/modules/contributed/",
        "api/modules/custom/"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
